{
  "name": "ipynb_ast",
  "version": "1.0.0",
  "description": "Converts notebooks to an AST much like md_ast in order to be able to manipulate and parse content for rendering pipelines",
  "homepage": "https://github.com/eduardo-escoto/ipynb_ast#readme",
  "bugs": {
    "url": "https://github.com/eduardo-escoto/ipynb_ast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardo-escoto/ipynb_ast.git"
  },
  "license": "ISC",
  "author": "Eduardo Escoto",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.22",
    "mdast-util-to-hast": "^13.2.0",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.8.1",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
